{
  "embedding": {
    "provider": "transformers",
    "transformers": {
      "model": "Xenova/all-MiniLM-L6-v2",
      "dimensions": 384,
      "pooling": "mean",
      "normalize": true
    },
    "ollama": {
      "baseUrl": "http://localhost:11434",
      "model": "all-minilm",
      "dimensions": 384,
      "maxConcurrent": 50,
      "options": {
        "temperature": 0
      }
    }
  },
  "processing": {
    "maxFileSizeBytes": 10485760,
    "tokenTarget": 500,
    "overlapTokens": 64,
    "concurrency": 4
  }
}
